clean: clean-eggs clean-build
	@find . -iname '*.pyc' -delete
	@find . -iname '*.pyo' -delete
	@find . -iname '*~' -delete
	@find . -iname '*.swp' -delete
	@find . -iname '__pycache__' -delete

test:
	pytest .

install:
	pip install -r requirements.txt

install-dev:
	pip install -r requirements-dev.txt
